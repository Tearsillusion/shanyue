* {
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}

body {
	margin: 0;
	padding: 0;
	overflow-x: hidden;
	background: #F5F5F5;

}

html {
	font-size: 100px;
	font-family: "Source Han Sans", 'PingFang SC', "Microsoft Yahei", "Helvetica Neue", Helvetica, Tahoma, sans-serif !important;
	overflow-x: hidden;
}


/* 头部
 */
.header {
	background: #FFFFFF;
}

.navbar {
	margin-bottom: 0;
	min-height: 0.86rem;
	max-width: 1300px;
	min-width: 950px;
	margin: 0 auto;
	border: none;
	background: #fff;

}

/* 首页导航 */

.navbar-brand {
	height: 0.84rem;
	line-height: 0.52rem;
}

.navbar-nav>li>a {
	line-height: 0.52rem;
}

.caret {
	border: none;
}

.nav>li {
	margin-right: 0.60rem;
}

.navbar-default .navbar-nav>.open>a,
.navbar-default .navbar-nav>.open>a:focus,
.navbar-default .navbar-nav>.open>a:hover {
	color: #d9231e;
	background-color: #fff;
	border-bottom: 0.02rem solid #d9231e;
}

.navbar-default .navbar-nav>.active>a,
.navbar-default .navbar-nav>.active>a:focus,
.navbar-default .navbar-nav>.active>a:hover {
	color: #d9231e;
	background-color: #fff;
	border-bottom: 0.02rem solid #d9231e;
}

.navbar-default .navbar-nav>li>a:focus,
.navbar-default .navbar-nav>li>a:hover {

	color: #d9231e;
	background-color: transparent;
	border-bottom: 0.02rem solid #d9231e;
}

.navbar-default .navbar-toggle:focus,
.navbar-default .navbar-toggle:hover {
	background-color: #fff;
}

.navbar-default .navbar-toggle {
	border: none;
	margin-right: 0.10rem;
}

.navbar-default .navbar-toggle {
	height: 0.70rem;
}

.dropdown-menu .divider {
	margin: 0.02rem 0;
}

/* 首页轮播图 */
.mainCarousel {
	width: 100%;
}

.carousel-inner {
	height: 100%;
}

.carousel-control.left {
	display: none;
}

.carousel-control.right {
	display: none;
}

.carousel-indicators .active {
	width: 0.60rem;
	height: 0.04rem;
	background-color: #D9231E;
}

.carousel-indicators li {
	width: 0.60rem;
	height: 0.04rem;
	background-color: #888;
	border: none;
}

.carousel-caption {
	top: 0;
}

.carousel-caption>h3 {
	margin-top: 0.80rem;
	font-size: 0.38rem;
}

.carousel-caption>h1 {
	font-size: 0.60rem;
	font-weight: 600;
	letter-spacing: 2px;
	margin-top: 0.30rem;
}

.carousel-caption>p {
	font-size: 0.18rem;
}

.carousel-caption>p:nth-of-type(1) {
	margin-top: 0.40rem;
}

/* 产品中心 */

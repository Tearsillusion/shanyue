/* 解决方案 */
.resolve {
	width: 99.38%;
	height: 6.6rem;
	background: url(../image/resolve.png) no-repeat;
	background-size: 100% 100%;
	margin: 0.80rem auto 0.80rem;
	text-align: center;
	padding-top: 0.30rem;
}

.resolve>.row {
	height: 100%;
}

.resolveModel {
	padding: 0;
}

.resolveModel {
	height: 100%;
}

.resolveTitle>h5 {
	font-size: 0.30rem;
	font-weight: 600;
	color: #FFFFFF;
}

.resolveTitle>p {
	font-size: 0.14rem;
	color: #BBBBBB;
}

.section-wrapper {
	width: 100%;
	margin: 0.40rem auto 0;
}

.plan-content {
	position: relative;
	height: 4.2rem;
	background-color: #000;
}

.plan-content:before,
.plan-content:after {
	content: " ";
	display: table;
}

.plan-container {
	float: left;
	height: 100%;
	width: 100%;
	overflow: hidden;
}

.plan-wrapper {
	position: relative;
	left: 0;
	height: 100%;
	font-size: 0;
	white-space: nowrap;
}

.plan-slide {
	display: inline-block;
	position: relative;
	height: 100%;
	width: 18%;
	background-repeat: no-repeat;
	background-position: center top;
	background-size: cover;
	font-size: 0.30rem;
	overflow: hidden;
	vertical-align: top;
	white-space: normal;
	transition: all .5s;
	-moz-transition: all .5s;
	-webkit-transition: all .5s;
}

.plan-slide:not(.open):hover .plan-card-bg:after {
	opacity: .2;
}

.plan-card-bg {
	position: relative;
	width: 100%;
	height: 100%;
	transition: .2s;
}

.plan-card-bg img {
	display: block;
	position: absolute;
	top: -10000px;
	right: -10000px;
	bottom: -10000px;
	left: -10000px;
	height: 100%;
	margin: auto;
}

.plan-card-bg:after {
	content: '';
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	background: #060606;
	opacity: .7;
	transition: .2s;
}

.plan-card-collapse {
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	cursor: pointer;
	transition: .5s;
}

.plan-card-collapse-inner {
	position: absolute;
	top: 50%;
	width: 100%;
	margin-top: -0.63rem;
	font-size: 0;
	text-align: center;
	letter-spacing: 1px;
}

.plan-card-collapse-inner i {
	width: 0.30rem;
	height: 0.02rem;
	display: block;
	background: #fd4750;
	margin: 0.13rem auto 0.19rem;
}

.plan-card-collapse-inner .h2 {
	font-size: 0.20rem;
	color: #fff;
	line-height: 0.30rem;
}

.plan-card-open {
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	cursor: pointer;
	opacity: 0;
	visibility: hidden;
}

.plan-card-open-inner {
	position: absolute;
	top: 50%;
	left: 13%;
	right: 13%;
	width: 54%;
	-ms-transform: translate(0, -50%);
	transform: translate(0, -50%);
	margin: auto;
	color: #fff;
	text-align: center;
	letter-spacing: 1px;
}

.plan-slide.open .plan-card-open {
	opacity: 1;
	visibility: visible;
}

.plan-card-open-inner .h1 {
	position: relative;
	margin-bottom: 0.50rem;
	font-size: 0.22rem;
	color: #fff;
	line-height: 0.38rem;
}

.plan-card-open-inner .h1:after {
	content: "";
	display: block;
	position: absolute;
	top: 100%;
	right: 0;
	left: 0;
	width: 0.74rem;
	height: 0.02rem;
	margin: 0.13rem auto 0;
	background-color: #fd4750;
}

.plan-card-open-inner p {
	margin-bottom: 0.49rem;
	font-size: 0.14rem;
	color: #fff;
	font-weight: 300;
	line-height: 0.24rem;
}

.plan-recommend {
	display: inline-block;
	font-size: 0.14rem;
	line-height: 0.24rem;
}

.plan-recommend h2 {
	color: rgba(255, 255, 255, .7);
	font-size: 0.14rem;
	line-height: 0.24rem;
}

.plan-recommend ul {
	margin-top: 0.08rem;
	height: auto;
	overflow: hidden;
	text-align: center;
}

.plan-recommend ul>li {
	display: inline-block;
	list-style-type: none;
}

.plan-recommend ul>li a {
	transition: .2s;
	color: #fff;
	font-size: 0.14rem;
	line-height: 0.24rem;
	text-decoration: none;
}

.plan-recommend ul>li+li:before {
	content: "\2002|\2002";
	opacity: .5;
}

.plan-slide.open {
	width: 46%;
}

.xsytb1 {
	display: inline-block;
	font-size: 0;
}

/* 手机版解决方案 */
/* 手动轮播 */
.hander_lunbo {
	width: 100%;
	position: relative;
}

.hander_lunbo>ul {
	width: 1000%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
}

.hander_lunbo>ul>li {
	height: 100%;
	float: left;
	margin: 0 0.10rem 0 0;
	position: relative;
}

.hander_lunbo>ul>li>a {
	display: block;
	width: 100%;
	height: 100%;
}

.hander_lunbo>ul>li img {
	width: 100%;
	height: 100%;
}

.hander_lunbo>ul>li h5 {
	width: 100%;
	line-height: 0.50rem;
	height: 0.50rem;
	font-size: 0.24rem;
	color: #FFFFFF;
	text-align: center;
	position: absolute;
	top: 0.30rem;
	left: 0;
	font-weight: 400;
}

.hander_lunbo>ul>li h6 {
	width: 100%;
	line-height: 0.30rem;
	height: 0.30rem;
	font-size: 0.12rem;
	color: #FFFFFF;
	text-align: center;
	position: absolute;
	top: 0.66rem;
	left: 0;
	font-weight: 400;
}

.swiper-slide img {
	width: 100%;
	border-radius: 0.10rem;
	display: block;
}

.swiper-slide a {
	background: none;
	display: inline-block;
	padding: 6px;
	display: block;
	border-radius: 0.10rem;
}

.swiper-slide h4 {
	width: 100%;
	text-align: center;
	position: absolute;
	left: 50%;
	top: 40%;
	transform: translate(-50%, -40%);
	font-size: 0.22rem;
	color: white;
	font-weight: 100;

}

.swiper-slide span {
	width: 100%;
	text-align: center;
	position: absolute;
	left: 50%;
	top: 54%;
	transform: translate(-50%, -54%);
	margin-top: 0.1rem;
	font-size: 0.16rem;
	color: white;
	font-weight: 100;

}

.mobileSwiper {
	margin-top: 0.50rem;
	margin-bottom: 0.50rem;
}

.mobileresolveTitle {
	text-align: center;
}

.mobileresolveTitle>h5 {
	font-size: 0.30rem;

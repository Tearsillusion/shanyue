body {
	margin: 0;
	padding: 0;
}

html {
	font-size: 100px;
}

/* 头部
 */
.header {
	background: #FFFFFF;
}

.navbar {
	margin-bottom: 0;
	min-height: 0.84rem;
	max-width: 1300px;
	min-width: 950px;
	margin: 0 auto;
	border: none;
	background: #fff;

}

/* 首页导航 */

.navbar-brand {
	height: 0.84rem;
	line-height: 0.52rem;
}

.navbar-nav>li>a {
	line-height: 0.52rem;
}

.caret {
	border: none;
}

.nav>li {
	margin-left: 0.40rem;
}

.navbar-default .navbar-nav>.open>a,
.navbar-default .navbar-nav>.open>a:focus,
.navbar-default .navbar-nav>.open>a:hover {
	color: #d9231e;
	background-color: #fff;
	border-bottom: 0.02rem solid #d9231e;
}

.navbar-default .navbar-nav>.active>a,
.navbar-default .navbar-nav>.active>a:focus,
.navbar-default .navbar-nav>.active>a:hover {
	color: #d9231e;
	background-color: #fff;
	border-bottom: 0.02rem solid #d9231e;
}

.navbar-default .navbar-nav>li>a:focus,
.navbar-default .navbar-nav>li>a:hover {

	color: #d9231e;
	background-color: transparent;
	border-bottom: 0.02rem solid #d9231e;
}

.navbar-default .navbar-toggle:focus,
.navbar-default .navbar-toggle:hover {
	background-color: #fff;
}

.navbar-default .navbar-toggle {
	border: none;
	margin-right: 0.10rem;
}

.navbar-default .navbar-toggle {
	height: 0.70rem;
}

.dropdown-menu .divider {
	margin: 0.02rem 0;
}

/* 巨幕 */
.scheme-banner {
	width: 100%;

}

.scheme-banner .jumbotron {
	text-align: center;
	background: url(../image/banner1.png) no-repeat;
	background-size: cover;
	background-position: center;
	color: white;
	padding: 1rem 0 1rem 0;
}

.schems-list-banner .jumbotron {
	background: url(../image/banner2.png) no-repeat;
}

.new-banner .jumbotron {
	background: url(../image/banner3.png) no-repeat;
}

/* 方案列表 */
.scheme-list .container {
	margin: 0 auto;
}

.scheme-list .thumbnail {
	position: relative;
	overflow: hidden;
	padding: 0;
}

.scheme-list-icon {
	display: block;
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	margin: auto;
	width: 50%;
	height: 50%;
}

.scheme-list-icon img {
	width: 0.8rem;
	height: 0.8rem;
	display: block;
	margin: 0 auto;
}

.scheme-list-icon p {
	text-align: center;
	color: white;
	font-size: 0.2rem;
	margin-top: 0.2rem;
	position: relative;
}

.scheme-list-icon p::after {
	content: "";
	width: 0.4rem;
	height: 0.1rem;
	border-bottom: 2px solid white;
	position: absolute;
	left: 0;
	right: 0;
	bottom: -0.5rem;
	top: 0;
	margin: auto;

}

.scheme-list-model {
	position: absolute;
	left: 0;
	top: 100%;
	width: 100%;
	height: 100%;
	z-index: 8;
	background: rgba(85, 100, 211, 0.8);
	font-size: 0;
}

.scheme-list-model h4 {
	color: white;
	font-size: 0.2rem;
	text-align: center;
	position: relative;
	margin-top: 1rem;
}

.scheme-list-model h4::after {
	content: "";
	width: 0.4rem;
	height: 0.1rem;
	border-bottom: 2px solid white;
	position: absolute;
	left: 0;
	right: 0;
	bottom: -0.5rem;
	top: 0;
	margin: auto;
}

.scheme-list-model p {
	color: white;
	opacity: 0.6;
	font-size: 0.15rem;
	cursor: pointer;
	padding: 0.2rem 0.5rem 0 0.5rem;
	box-sizing: border-box;
	text-align: center;

}

.scheme-list-model p:hover {
	opacity: 1;
}

/* 底部 */
.scheme-footer {
	width: 100%;
	background: black;
	padding-top: 0.3rem;
}

.scheme-footer .container {
	margin: 0 auto;
}

.scheme-footer-left {
	color: white;
}

.scheme-footer-left h6 {
	font-size: 0.16rem;
	opacity: 0.8;
	width: 2rem;
	padding-bottom: 0.1rem;
}

.scheme-footer-left p {
	font-size: 0.14rem;
	opacity: 0.6;
}

.scheme-footer-right-list {
	color: white;
}

.scheme-footer-right-list h6 {
	font-size: 0.16rem;
	opacity: 0.8;
	width: 2rem;
	padding-bottom: 0.1rem;
}

.scheme-footer-right-list a {
	font-size: 0.14rem;
	opacity: 0.6;
	color: white;
	display: block;
	margin-bottom: 0.1rem;
	cursor: pointer;
}

.scheme-footer-look {}

.scheme-footer-look h6 {
	font-size: 0.16rem;
	opacity: 0.8;
	width: 2rem;
	padding-bottom: 0.1rem;
	color: white;


}

.scheme-footer-look img {
	width: 1rem;
	height: 1rem;

}

.scheme-footer-copyright {
	width: 100%;
	padding: 0.2rem;
	border-top: 1px solid #555;
	font-size: 0.14rem;
	color: white;
	opacity: 0.6;
	text-align: center;
	clear: both;
	margin-top: 0.5rem;
	display: inline-block;
}

@media (max-width:768px) {
	.scheme-footer-left {
		text-align: center;
	}

	.scheme-footer-left h6 {
		text-align: center;
		width: 100%;
	}

	.scheme-footer-look {
		text-align: center;
	}

	.scheme-footer-look h6 {
		text-align: center;
		width: 100%;
	}
}

